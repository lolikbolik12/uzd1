#include<iostream>;
using namespace std;

void ceturtaisuzd() {
	string password="qce";
	string i;
	int c=1;
	do {
		cout<<"Enter password:";
		cin>>i;
		if(i!=password) {
			c++;
			cout<<"Try again"<<endl;
		}
		else {
			cout<<"Password is corect!";
			//return 0;
		};
	} while(c!=6);
}

void piektaisuzd() {
	int i=1000;
	do {
		cout<<i-5<<endl;
		i=i-5;
	} while(i>0);
}
void time() {
	int time;
	cout<<"Enter time:";
	cin>>time;
	if(time<=12) {
		cout<<time<<"AM"<<endl;
	}
	else if(time>12 && time<=24) {
		cout<<time-12<<"PM"<<endl;
	}
	else {
		cout<<"You entered incorect time!"<<endl;
	}
}

int main() {
	int i1,i2,i3;
	cout<<"Enter number 1:";
	cin>>i1;
	cout<<"Enter number 2:";
	cin>>i2;
	cout<<"Enter number 3:";
	cin>>i3;

	if(i1>i2 && i1>i3) {
		cout<<i1<<"-is max number"<<endl;
	}
	else if(i1<i2 && i1<i3) {
		cout<<i1<<"-is min number"<<endl;
	}
	if(i2>i1 && i2>i3) {
		cout<<i2<<"-is max number"<<endl;
	}
	else if(i2<i1 && i2<i3) {
		cout<<i2<<"-is min number"<<endl;
	}
	 if(i3>i1 && i3>i2) {
		cout<<i3<<"-is max number"<<endl;
	}
	else if(i3<i1 && i3<i2) {
		cout<<i3<<"-is min number"<<endl;
	}
}
